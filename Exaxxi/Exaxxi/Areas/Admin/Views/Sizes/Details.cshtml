@model Exaxxi.Models.Sizes

@{
    ViewData["Title"] = "Details";
}
<div class="row" ng-controller="editDsSize">
    <div class="col-md-8">
        <div class="card">
            <div class="card-header card-header-primary">
                <h4 class="card-title">Chi Tiết</h4>
                <p class="card-category">Sizes</p>
            </div>
            <div class="card-body">
                <form>
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="lowest_ask" class="bmd-label-floating"></label>
                        <input asp-for="lowest_ask" class="form-control" disabled />
                        <span asp-validation-for="lowest_ask" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="highest_bid" class="bmd-label-floating"></label>
                        <input asp-for="highest_bid" class="form-control" disabled />
                        <span asp-validation-for="highest_bid" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="last_sale" class="bmd-label-floating"></label>
                        <input asp-for="last_sale" class="form-control" disabled />
                        <span asp-validation-for="last_sale" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="size.VN" class="bmd-label-floating"></label>
                        <input asp-for="size.VN" class="form-control" disabled />
                        <span asp-validation-for="size.VN" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="item" class="bmd-label-floating"></label>
                        <input asp-for="item.name" class="form-control" disabled />
                        <span asp-validation-for="item.name" class="text-danger"></span>
                    </div>
                    <a asp-action="Index">Quay Về</a>
                </form>
            </div>
        </div>
    </div>
</div>
